<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:camel="http://camel.apache.org/schema/spring"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/util
    http://www.springframework.org/schema/util/spring-util-3.0.xsd
	http://activemq.apache.org/camel/schema/spring
	http://activemq.apache.org/camel/schema/spring/camel-spring.xsd">

	<!-- ====================================== -->
	<!-- Enumerated Request Attribute Constants -->
	<!-- ====================================== -->

	<util:constant id="queryResultAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.QUERY_RESULT" />

	<util:constant id="resultPagerAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.RESULT_PAGER" />

	<util:constant id="pagingControllerAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.PAGING_LOOP_CONTROLLER" />

	<util:constant id="pagingNumHeaderRowsAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.PAGING_NUM_HEADER_ROWS" />

	<util:constant id="pagingPageSizeAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.PAGING_PAGE_SIZE" />

	<util:constant id="marshallSourceAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.MARSHAL_OBJ" />

	<util:constant id="marshallResultAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.MARSHAL_RSLT" />

	<util:constant id="marshalSchemaAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.MARSHAL_SCHEMA" />

	<util:constant id="resultSchemaAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.RESULT_SCHEMA" />

	<util:constant id="querySchemaAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.QUERY_SCHEMA" />

	<util:constant id="unmarshallSourceAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.UNMARSHAL_SRC" />

	<util:constant id="unmarshallResultAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.UNMARSHAL_RSLT" />

	<util:constant id="unmarshallPkgAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.UNMARSHAL_PKG" />

	<util:constant id="persistentEntitiesAttr"
		static-field="edu.utah.further.ds.api.util.AttributeName.PERSIST_ENTITIES" />

</beans>